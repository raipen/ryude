{
  "name": "ryude",
  "version": "1.0.0",
  "description": "node.js framework generating a markdown blog with React",
  "main": "dist/src/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "test": "npm run build && npm run typescript",
    "test:build": "npm run build && npm run ryude-build",
    "build": "rimraf ./dist && tsc",
    "typescript": "tsd",
    "start": "node ./dist/src/index.js",
    "ryude-build": "node ./dist/src/index.js build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raipen/ryude.git"
  },
  "engines": {
    "node": ">=14.16"
  },
  "keywords": [
    "ryude",
    "react",
    "blog",
    "SSG",
    "static site generator"
  ],
  "author": "Raipen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/raipen/ryude/issues"
  },
  "homepage": "https://github.com/raipen/ryude#readme",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/node": "^20.10.4",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "tsd": "^0.29.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
